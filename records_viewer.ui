<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="ttk.Frame" id="mainFrame">
    <property name="height">600</property>
    <property name="width">800</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Frame" id="csvFrame">
        <property name="borderwidth">5</property>
        <property name="height">500</property>
        <property name="relief">ridge</property>
        <property name="width">800</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
        <child>
          <object class="ttk.Label" id="label3">
            <property name="anchor">center</property>
            <property name="text" translatable="yes">No data loaded yet!</property>
            <layout manager="place">
              <property name="anchor">center</property>
              <property name="x">400</property>
              <property name="y">245</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="fetchButton">
        <property name="command" type="command" cbtype="simple">onFetchButtonClick</property>
        <property name="default">normal</property>
        <property name="text" translatable="yes">Fetch data</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.0</property>
          <property name="rely">0.0</property>
          <property name="x">320</property>
          <property name="y">535</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tkcalendarwidgets.dateentry" id="calendarStart">
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.0</property>
          <property name="rely">0.0</property>
          <property name="x">75</property>
          <property name="y">520</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tkcalendarwidgets.dateentry" id="calendarEnd">
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relx">0.0</property>
          <property name="rely">0.0</property>
          <property name="x">75</property>
          <property name="y">560</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label1">
        <property name="text" translatable="yes">Start date:</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="x">5</property>
          <property name="y">520</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="label2">
        <property name="text" translatable="yes">End date:</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="x">5</property>
          <property name="y">560</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Radiobutton" id="hourlyOpt">
        <property name="state">normal</property>
        <property name="text" translatable="yes">Hourly data</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="x">180</property>
          <property name="y">520</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Radiobutton" id="dailyOpt">
        <property name="state">normal</property>
        <property name="text" translatable="yes">Daily data</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="x">180</property>
          <property name="y">540</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Radiobutton" id="monthlyOpt">
        <property name="state">normal</property>
        <property name="text" translatable="yes">Monthly data</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="x">180</property>
          <property name="y">560</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
